__m512d tmpMtr[3][2];

 for ( int r=0, rsz=0, rpackoffset = 0; r<3; ++r, rpackoffset += rsz) { 
tmpMtr[r][0] = _mm512_mask_load_pd(tmpMtr[r][0], 0xFF, ((rBuf)+rpackoffset));

tmpMtr[r][1] = _mm512_mask_load_pd(tmpMtr[r][1], 0xFF, (((rBuf)+rpackoffset)+8));

rsz = 2*VECLEN;

}
 for ( int c1=0; c1<3; c1+=1 ) {
_mm512_stream_pd((void*)(*goBase)[c1][0],tmpMtr[c1][0]);

_mm512_stream_pd((void*)(*goBase)[c1][1],tmpMtr[c1][1]);

}
